{
  "name": "xparse/css-expression-translator",
  "description": "Translate css to xpath",
  "keywords": [],
  "homepage": "https://github.com/xparse/CssExpressionTranslator",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Shcherbak @funivan",
      "email": "alotofall@gmail.com",
      "homepage": "http://funivan.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/css-selector": "^5.2",
    "xparse/expression-translator": "0.0.* || ^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Xparse\\CssExpressionTranslator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xparse\\CssExpressionTranslator\\Test\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
